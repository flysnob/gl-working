<div class="container">
  <div class="row">
    <h3>Projects</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @projects.each do |project| %>
            <tr>
              <%= render project %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
